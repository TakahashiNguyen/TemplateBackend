{
	"debug.javascript.autoAttachFilter": "disabled",
	"editor.detectIndentation": false,
	"editor.foldingImportsByDefault": true,
	"editor.insertSpaces": false,
	"editor.tabSize": 2,
	"explorerExclude.backup": {},
	"files.eol": "\n",
	"files.exclude": {
		"**/*.example*": true,
		"**/*.js": true,
		"**/*.json": false,
		"**/*.tsbuildinfo": true,
		"**/*spec*": true,
		"**/.DS_Store": true,
		"**/.git": true,
		"**/.hg": true,
		"**/.svn": true,
		"**/CVS": true,
		"**/Thumbs.db": true,
		"**/schema.gql": true,
		".adminjs": true,
		".devcontainer": true,
		".env": true,
		".git": true,
		".github": true,
		".gitignore": true,
		".prettierrc": true,
		"LICENSE": true,
		"coverage": true,
		"dist": true,
		"node_modules": true,
		"reports": true,
		"secrets": true
	},
	"jest.outputConfig": {
		"clearOnRun": "terminal"
	},
	"jest.runMode": {
		"coverage": true,
		"showInlineError": true,
		"type": "on-demand"
	},
	"testing.openTesting": "neverOpen",
	"todohighlight.keywords": [
