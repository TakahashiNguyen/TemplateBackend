		"@nestjs/graphql": "^12.2.0",
		"@nestjs/jwt": "^10.2.0",
		"@nestjs/passport": "^10.0.3",
		"@nestjs/platform-express": "^10.0.0",
		"@nestjs/typeorm": "^10.0.2",
		"adminjs": "^7.8.11",
		"bcrypt": "^5.1.1",
		"class-transformer": "^0.5.1",
		"class-validator": "^0.14.1",
		"cookie-parser": "^1.4.6",
		"crypto": "^1.0.1",
		"express": "^4.19.2",
		"geojson": "^0.5.0",
		"graphql": "^16.9.0",
		"http": "^0.0.1-security",
		"https": "^1.0.0",
		"joi": "^17.13.3",
		"passport": "^0.7.0",
		"passport-jwt": "^4.0.1",
