{
	"name": "templatebackend",
	"version": "0.0.1",
	"private": true,
	"license": "MIT",
	"author": "Nguyễn Việt Anh <takahashi.ng@icloud.com>",
	"type": "commonjs",
	"main": "types.js",
	"types": "types.d.ts",
	"scripts": {
		"buildfile": "ts-node src/build.ts",
		"prebuild": "npm install && npm run buildfile",
		"build": "nest build",
		"format": "replace '{\n' '{' . -r --include=\"*.ts,*.js*\" --exclude=\"node_modules\" --silent && replace \"\\[\n\" \"[\" . -r --include=\"*.ts,*.js*\" --exclude=\"node_modules\" --silent && prettier --write \"./**/*.ts\" \"./**/*.js*\" \"./**/*.*rc\" --log-level silent",
		"lint": "eslint --fix  --ext .ts .",
		"safe": "generate-env-example",
		"start": "nest start",
		"prestart": "npm run build",
		"start:debug": "nest start --debug --watch",
		"start:dev": "nest start --watch",
		"start:prod": "node dist/main",
		"pretest": "npm run build",
		"test": "jest",
		"test:debug": "jest --debug",
		"types": "tsc --project types/tsconfig.build.json --outDir types  --rootDir . --sourceMap false",
		"pretypes": "npm run prebuild",
		"triathlon": "npm run test && npm run format && npm run lint"
	},
	"dependencies": {
		"@adminjs/express": "^6.1.0",
		"@adminjs/typeorm": "^5.0.1",
		"@nestjs/common": "^10.0.0",
